<%= simple_form_for(@revision) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :document %>
    <%= f.input :content %>
    <%= f.input :mimetype %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
