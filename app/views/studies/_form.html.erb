<%= simple_form_for(@study) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :organization %>
    <%= f.input :degree %>
    <%= f.input :content %>
    <%= f.input :started_at %>
    <%= f.input :finished_at %>
    <%= f.input :is_current %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
