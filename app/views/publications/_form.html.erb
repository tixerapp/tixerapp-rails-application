<%= simple_form_for(@publication) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :organization %>
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :published_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
